{
  "manifest_version": 2,

  "name": "ChromeTime Plugin",
  "description": "This extension will show you current time",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
   "permissions": [
	"activeTab",
	"webRequest",
	"webRequestBlocking",
	"tabs",
	"<all_urls>"
]
}